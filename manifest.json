{
    "name":"Phishing Detection",
    "version":"1.0.0",
    "manifest_version": 2,
    "description" : "This extension allows you to securely browse throughout the internet without worring about malicious phishing websites",
    "icons" : {
      "16" : "images/logo_16.png",
      "48" : "images/logo_48.png",
      "128": "images/logo_128.png" 
    },
    "browser_action":{
        "default_popup":"popup.html"
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "permissions":[
      "activeTab",
      "tabs"
    ]
    
}